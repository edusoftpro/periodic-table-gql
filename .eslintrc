{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "plugins": [
        "@typescript-eslint",
        "import"
    ],
    "extends": "eslint:recommended",
    "rules": {
        "eqeqeq": 2,
        "no-alert": 2,
        "no-console": 2,
        "no-constant-condition": 2,
        "no-debugger": 2,
        "no-duplicate-imports": 2,
        "no-eq-null": 2,
        "no-irregular-whitespace": 2,
        "no-magic-numbers": 2,
        "no-undef": 2,
        "no-unused-expressions": 2,
        "no-use-before-define": 2,
        "no-var": 2
    }
}